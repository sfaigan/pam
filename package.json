{
  "name": "pam",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "client:prod": "cd client && npm run start",
    "client:dev": "cd client && npm run start",
    "server:prod": "cd server && npm run start",
    "server:dev": "cd server && npm run dev",
    "start": "concurrently \"npm run server:prod\" \"npm run client:prod\"",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "lint": "eslint \"**/src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup:users": "python3 seeds/generators/users.py && mongoimport --db \"pam\" --collection \"users\" --type json --file \"users.json\" --jsonArray --drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfaigan/PAM.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "nanoid": "^3.1.20"
  }
}
